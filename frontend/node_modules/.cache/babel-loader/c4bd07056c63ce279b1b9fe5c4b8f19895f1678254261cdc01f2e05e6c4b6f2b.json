       no2: '38μg/m³'\n      }, {\n        station: 'S3',\n        pm25: '31μg/m³',\n        pm10: '62μg/m³',\n        no2: '45μg/m³'\n      }]\n    }]);\n\n    // 模拟图片数据\n    function generatePhotos() {\n      const photos = [];\n      const types = ['aerial', 'soil', 'water', 'vegetation'];\n      const locations = ['A', 'B', 'C', 'D'];\n      for (let i = 1; i <= 12; i++) {\n        const type = types[Math.floor(Math.random() * types.length)];\n        const location = locations[Math.floor(Math.random() * locations.length)];\n        const month = String(i).padStart(2, '0');\n        const day = String(Math.floor(Math.random() * 28) + 1).padStart(2, '0');\n        photos.push({\n          name: `${type}_${location}_2024${month}${day}.jpg`,\n          date: `2024-${month}-${day}`,\n          url: `https://picsum.photos/400/300?random=${i}`,\n          // 使用随机图片服务\n          type: type,\n          location: location\n        });\n      }\n      return photos;\n    }\n    const allPhotos = ref(generatePhotos());\n\n    // 预览相关\n    const previewVisible = ref(false);\n    const previewData = ref(null);\n    const previewColumns = ref([]);\n    const photoPreviewVisible = ref(false);\n    const currentPreviewPhoto = ref('');\n\n    // 处理函数\n    function handleUpload(file) {\n      console.log('上传文件:', file.name);\n      alert('上传功能待实现: ' + file.name);\n    }\n    function preview(row) {\n      previewData.value = row.preview;\n      previewColumns.value = Object.keys(row.preview[0]);\n      previewVisible.value = true;\n    }\n    function download(row) {\n      console.log('下载文件:', row.name);\n      alert('下载功能待实现: ' + row.name);\n    }\n    function remove(row) {\n      dataList.value = dataList.value.filter(item => item !== row);\n    }\n    function previewPhoto(photo) {\n      currentPreviewPhoto.value = photo.url;\n      photoPreviewVisible.value = true;\n    }\n    function deletePhoto(photo) {\n      allPhotos.value = allPhotos.value.filter(item => item !== photo);\n    }\n    function getTagType(type) {\n      const types = {\n        'Excel': 'success',\n        'CSV': 'warning',\n        'JPG': 'info',\n        'PNG': 'info'\n      };\n      return types[type] || 'info';\n    }\n    const __returned__ = {\n      totalFiles,\n      monthlyUploads,\n      storageUsed,\n      dataList,\n      generatePhotos,\n      allPhotos,\n      previewVisible,\n      previewData,\n      previewColumns,\n      photoPreviewVisible,\n      currentPreviewPhoto,\n      handleUpload,\n      preview,\n      download,\n      remove,\n      previewPhoto,\n      deletePhoto,\n      getTagType,\n      ref\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","totalFiles","monthlyUploads","storageUsed","dataList","name","type","date","size","preview","point","ph","moisture","metal","temp","humidity","location","turbidity","tds","station","pm25","pm10","no2","generatePhotos","photos","types","locations","i","Math","floor","random","length","month","String","padStart","day","push","url","allPhotos","previewVisible","previewData","previewColumns","photoPreviewVisible","currentPreviewPhoto","handleUpload","file","console","log","alert","row","value","Object","keys","download","remove","filter","item","previewPhoto","photo","deletePhoto","getTagType"],"sources":["D:/restore/water-melt/frontend/src/views/DataManagement.vue"],"sourcesContent":["<!-- DataManagement.vue -->\r\n<template>\r\n  <div class=\"data-management-view\">\r\n    <h2>数据管理</h2>\r\n    \r\n    <!-- 数据统计卡片 -->\r\n    <div class=\"stat-cards\">\r\n      <div class=\"stat-card\">\r\n        <div class=\"stat-title\">总文件数</div>\r\n        <div class=\"stat-value\">{{ totalFiles }}</div>\r\n      </div>\r\n      <div class=\"stat-card\">\r\n        <div class=\"stat-title\">本月上传</div>\r\n        <div class=\"stat-value\">{{ monthlyUploads }}</div>\r\n      </div>\r\n      <div class=\"stat-card\">\r\n        <div class=\"stat-title\">存储空间</div>\r\n        <div class=\"stat-value\">{{ storageUsed }}</div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 上传组件 -->\r\n    <el-upload\r\n      class=\"upload-demo\"\r\n      drag\r\n      action=\"#\"\r\n      :show-file-list=\"false\"\r\n      :on-change=\"handleUpload\"\r\n      style=\"margin-bottom: 20px;\"\r\n    >\r\n      <i class=\"el-icon-upload\"></i>\r\n      <div class=\"el-upload__text\">将文件拖到此处，或<em>点击上传</em></div>\r\n      <div class=\"upload-tip\">支持 .xlsx, .csv, .jpg, .png 格式文件</div>\r\n    </el-upload>\r\n\r\n    <!-- 数据列表 -->\r\n    <div class=\"section-title\">监测数据文件</div>\r\n    <el-table :data=\"dataList\" style=\"width: 100%; margin-bottom: 30px;\">\r\n      <el-table-column prop=\"name\" label=\"文件名\" />\r\n      <el-table-column prop=\"type\" label=\"类型\" width=\"120\">\r\n        <template #default=\"scope\">\r\n          <el-tag :type=\"getTagType(scope.row.type)\">{{ scope.row.type }}</el-tag>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"date\" label=\"上传日期\" width=\"180\" />\r\n      <el-table-column prop=\"size\" label=\"大小\" width=\"120\" />\r\n      <el-table-column label=\"操作\" width=\"200\">\r\n        <template #default=\"scope\">\r\n          <el-button size=\"small\" @click=\"preview(scope.row)\">预览</el-button>\r\n          <el-button size=\"small\" @click=\"download(scope.row)\">下载</el-button>\r\n          <el-button size=\"small\" type=\"danger\" @click=\"remove(scope.row)\">删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!-- 图片资源展示 -->\r\n    <div class=\"section-title\">监测图片资源</div>\r\n    <div class=\"photo-grid\">\r\n      <div v-for=\"file in allPhotos\" :key=\"file.path\" class=\"photo-item\">\r\n        <img :src=\"file.url\" :alt=\"file.name\" class=\"photo-img\" />\r\n        <div class=\"photo-info\">\r\n          <div class=\"photo-name\">{{ file.name }}</div>\r\n          <div class=\"photo-date\">{{ file.date }}</div>\r\n          <div class=\"photo-actions\">\r\n            <el-button size=\"small\" @click=\"previewPhoto(file)\">查看</el-button>\r\n            <el-button size=\"small\" type=\"danger\" @click=\"deletePhoto(file)\">删除</el-button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 预览对话框 -->\r\n    <el-dialog v-model=\"previewVisible\" title=\"数据预览\" width=\"80%\">\r\n      <div v-if=\"previewData\" class=\"preview-content\">\r\n        <el-table :data=\"previewData\" border style=\"width: 100%\">\r\n          <el-table-column v-for=\"col in previewColumns\" :key=\"col\" :prop=\"col\" :label=\"col\" />\r\n        </el-table>\r\n      </div>\r\n    </el-dialog>\r\n\r\n    <!-- 图片预览对话框 -->\r\n    <el-dialog v-model=\"photoPreviewVisible\" width=\"60%\">\r\n      <img :src=\"currentPreviewPhoto\" style=\"width: 100%\" />\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref } from 'vue'\r\n\r\n// 统计数据\r\nconst totalFiles = ref(156)\r\nconst monthlyUploads = ref(23)\r\nconst storageUsed = ref('2.8 GB')\r\n\r\n// 数据文件列表\r\nconst dataList = ref([\r\n  { \r\n    name: 'soil_data_2024Q1.xlsx', \r\n    type: 'Excel', \r\n    date: '2024-03-15 14:30', \r\n    size: '2.5MB',\r\n    preview: [\r\n      { point: 'A1', ph: 6.8, moisture: '23%', metal: '0.03mg/L' },\r\n      { point: 'A2', ph: 7.1, moisture: '21%', metal: '0.02mg/L' },\r\n      { point: 'A3', ph: 6.9, moisture: '22%', metal: '0.04mg/L' }\r\n    ]\r\n  },\r\n  { \r\n    name: 'monitoring_202403.csv', \r\n    type: 'CSV', \r\n    date: '2024-03-10 09:15', \r\n    size: '1.8MB',\r\n    preview: [\r\n      { date: '2024-03-10', temp: '23.5°C', humidity: '65%' },\r\n      { date: '2024-03-11', temp: '24.1°C', humidity: '63%' },\r\n      { date: '2024-03-12', temp: '22.8°C', humidity: '68%' }\r\n    ]\r\n  },\r\n  { \r\n    name: 'water_quality_202402.xlsx', \r\n    type: 'Excel', \r\n    date: '2024-02-28 16:45', \r\n    size: '3.2MB',\r\n    preview: [\r\n      { location: 'W1', ph: 7.2, turbidity: '0.8 NTU', tds: '180mg/L' },\r\n      { location: 'W2', ph: 7.4, turbidity: '1.2 NTU', tds: '165mg/L' },\r\n      { location: 'W3', ph: 7.1, turbidity: '0.9 NTU', tds: '175mg/L' }\r\n    ]\r\n  },\r\n  { \r\n    name: 'air_quality_202401.csv', \r\n    type: 'CSV', \r\n    date: '2024-01-31 11:20', \r\n    size: '2.1MB',\r\n    preview: [\r\n      { station: 'S1', pm25: '35μg/m³', pm10: '68μg/m³', no2: '42μg/m³' },\r\n      { station: 'S2', pm25: '28μg/m³', pm10: '55μg/m³', no2: '38μg/m³' },\r\n      { station: 'S3', pm25: '31μg/m³', pm10: '62μg/m³', no2: '45μg/m³' }\r\n    ]\r\n  }\r\n])\r\n\r\n// 模拟图片数据\r\nfunction generatePhotos() {\r\n  const photos = []\r\n  const types = ['aerial', 'soil', 'water', 'vegetation']\r\n  const locations = ['A', 'B', 'C', 'D']\r\n  \r\n  for (let i = 1; i <= 12; i++) {\r\n    const type = types[Math.floor(Math.random() * types.length)]\r\n    const location = locations[Math.floor(Math.random() * locations.length)]\r\n    const month = String(i).padStart(2, '0')\r\n    const day = String(Math.floor(Math.random() * 28) + 1).padStart(2, '0')\r\n    \r\n    photos.push({\r\n      name: `${type}_${location}_2024${month}${day}.jpg`,\r\n      date: `2024-${month}-${day}`,\r\n      url: `https://picsum.photos/400/300?random=${i}`, // 使用随机图片服务\r\n      type: type,\r\n      location: location\r\n    })\r\n  }\r\n  return photos\r\n}\r\n\r\nconst allPhotos = ref(generatePhotos())\r\n\r\n// 预览相关\r\nconst previewVisible = ref(false)\r\nconst previewData = ref(null)\r\nconst previewColumns = ref([])\r\nconst photoPreviewVisible = ref(false)\r\nconst currentPreviewPhoto = ref('')\r\n\r\n// 处理函数\r\nfunction handleUpload(file) {\r\n  console.log('上传文件:', file.name)\r\n  alert('上传功能待实现: ' + file.name)\r\n}\r\n\r\nfunction preview(row) {\r\n  previewData.value = row.preview\r\n  previewColumns.value = Object.keys(row.previe