{"ast":null,"code":"import { ref, computed, onMounted, onBeforeUnmount, watch, nextTick } from 'vue';\nimport BaseEChart from '@/components/BaseEChart.vue';\nimport { DataLine, Warning, Location, Check, ArrowUp, ArrowDown, Setting, Refresh, Search, CircleClose } from '@element-plus/icons-vue';\nimport * as echarts from 'echarts';\n\n// 监测统计数据\n\nexport default {\n  __name: 'Analysis',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const monitorStats = ref([{\n      value: 256,\n      label: '今日监测数据',\n      type: 'primary',\n      icon: DataLine,\n      trend: 12,\n      format: '0,0'\n    }, {\n      value: 5,\n      label: '异常数据',\n      type: 'warning',\n      icon: Warning,\n      trend: -5,\n      format: '0,0'\n    }, {\n      value: 32,\n      label: '监测点总数',\n      type: 'info',\n      icon: Location,\n      trend: 2,\n      format: '0,0'\n    }, {\n      value: 98,\n      label: '正常率',\n      type: 'success',\n      icon: Check,\n      trend: 3,\n      format: '0.00'\n    }]);\n\n    // 监测点选择\n    const selectedPoint = ref('point1');\n    const monitorPoints = [{\n      label: '监测点A',\n      value: 'point1'\n    }, {\n      label: '监测点B',\n      value: 'point2'\n    }, {\n      label: '监测点C',\n      value: 'point3'\n    }];\n\n    // 实时数据\n    const realtimeData = ref([{\n      label: '温度',\n      value: 25.6,\n      unit: '°C',\n      icon: Location,\n      isAbnormal: false,\n      trend: '上升',\n      trendData: [23.1, 24.2, 24.8, 25.1, 25.6]\n    }, {\n      label: '湿度',\n      value: 65.2,\n      unit: '%',\n      icon: Warning,\n      isAbnormal: true,\n      trend: '稳定',\n      trendData: [65.0, 65.1, 65.2, 65.2, 65.2]\n    }, {\n      label: 'PH值',\n      value: 6.8,\n      unit: '',\n      icon: Check,\n      isAbnormal: false,\n      trend: '下降',\n      trendData: [7.2, 7.0, 6.9, 6.8, 6.8]\n    }]);\n\n    // 图表加载状态\n    const isChartReady = ref(false);\n\n    // 获取小图表配置\n    const getChartOptions = item => ({\n      animation: false,\n      // 禁用动画以提高性能\n      grid: {\n        top: 0,\n        right: 0,\n        bottom: 0,\n        left: 0\n      },\n      xAxis: {\n        type: 'category',\n        show: false,\n        boundaryGap: false\n      },\n      yAxis: {\n        type: 'value',\n        show: false\n      },\n      series: [{\n        data: item.trendData,\n        type: 'line',\n        smooth: true,\n        showSymbol: false,\n        lineStyle: {\n          color: item.isAbnormal ? '#f56c6c' : '#67c23a'\n        },\n        areaStyle: {\n          color: new echarts.graphic.LinearGradient(0, 0, 0, 1, [{\n            offset: 0,\n            color: item.isAbnormal ? 'rgba(245, 108, 108, 0.2)' : 'rgba(103, 194, 58, 0.2)'\n          }, {\n            offset: 1,\n            color: 'rgba(255, 255, 255, 0)'\n          }])\n        }\n      }]\n    });\n\n    // 趋势分析\n    const trendTimeRange = ref('day');\n    const trendChartOptions = computed(() => ({\n      animation: false,\n      // 禁用动画以提高性能\n      tooltip: {\n        trigger: 'axis',\n        axisPointer: {\n          type: 'cross'\n        }\n      },\n      legend: {\n        data: ['温度', '湿度', 'PH值']\n      },\n      grid: {\n        left: '3%',\n        right: '4%',\n        bottom: '3%',\n        containLabel: true\n      },\n      xAxis: {\n        type: 'category',\n        boundaryGap: false,\n        data: ['00:00', '06:00', '12:00', '18:00', '24:00']\n      },\n      yAxis: [{\n        type: 'value',\n        name: '温度(°C)/湿度(%)',\n        position: 'left'\n      }, {\n        type: 'value',\n        name: 'PH值',\n        position: 'right'\n      }],\n      series: [{\n        name: '温度',\n        type: 'line',\n        smooth: true,\n        data: [22, 23, 25, 26, 25],\n        itemStyle: {\n          color: '#409eff'\n        }\n      }, {\n        name: '湿度',\n        type: 'line',\n        smooth: true,\n        data: [65, 64, 65, 66, 65],\n        itemStyle: {\n          color: '#67c23a'\n        }\n      }, {\n        name: 'PH值',\n        type: 'line',\n        smooth: true,\n        yAxisIndex: 1,\n        data: [7.2, 7.0, 6.9, 6.8, 6.8],\n        itemStyle: {\n          color: '#e6a23c'\n        }\n      }]\n    }));\n\n    // 监测点列表\n    const searchKeyword = ref('');\n    const filteredMonitorList = computed(() => {\n      const keyword = searchKeyword.value.toLowerCase();\n      return [{\n        name: '监测点A',\n        type: '农田',\n        value: '正常',\n        status: 'Normal',\n        trend: '稳定',\n        lastUpdate: '2分钟前'\n      }, {\n        name: '监测点B',\n        type: '水源',\n        value: '异常',\n        status: 'Warning',\n        trend: '上升',\n        lastUpdate: '1分钟前'\n      }].filter(item => item.name.toLowerCase().includes(keyword) || item.type.toLowerCase().includes(keyword));\n    });\n\n    // 告警列表\n    const alerts = ref([{\n      id: 1,\n      title: '监测点A12水质超标',\n      time: '10分钟前',\n      location: '西区-A12',\n      level: 'severe'\n    }, {\n      id: 2,\n      title: '监测点B23土壤重金属含量异常',\n      time: '30分钟前',\n      location: '东区-B23',\n      level: 'moderate'\n    }, {\n      id: 3,\n      title: '监测点C45数据传输异常',\n      time: '1小时前',\n      location: '南区-C45',\n      level: 'light'\n    }]);\n\n    // 工具函数\n    const getStatusIcon = status => {\n      const icons = {\n        'Normal': Check,\n        'Warning': Warning,\n        'Error': CircleClose\n      };\n      return icons[status] || Warning;\n    };\n    const getStatusType = status => {\n      const types = {\n        'Normal': 'success',\n        'Warning': 'warning',\n        'Error': 'danger'\n      };\n      return types[status] || 'info';\n    };\n    const getTrendIcon = trend => {\n      if (trend === '上升') return ArrowUp;\n      if (trend === '下降') return ArrowDown;\n      return Check;\n    };\n\n    // 在script setup部分添加错误处理函数\n    const handleChartError = error => {\n      console.error('图表渲染错误:', error);\n      // 可以在这里添加错误通知或其他处理逻辑\n    };\n\n    // 数据刷新\n    const refreshData = async () => {\n      try {\n        // 这里添加实际的数据获取逻辑\n        // const response = await api.getMonitorData(selectedPoint.value)\n        // realtimeData.value = response.data\n        isChartReady.value = false;\n        await nextTick();\n        isChartReady.value = true;\n      } catch (error) {\n        console.error('获取监测数据失败:', error);\n      }\n    };\n\n    // 设置面板\n    const showSettings = () => {\n      // 实现设置面板逻辑\n    };\n\n    // 选择监测点\n    const selectMonitorPoint = point => {\n      selectedPoint.value = point.name;\n      refreshData();\n    };\n\n    // 定时更新\n    let updateInterval;\n    onMounted(() => {\n      refreshData();\n      updateInterval = setInterval(refreshData, 60000); // 每分钟更新一次\n    });\n    onBeforeUnmount(() => {\n      if (updateInterval) {\n        clearInterval(updateInterval);\n      }\n      isChartReady.value = false;\n    });\n\n    // 监听监测点切换\n    watch(selectedPoint, () => {\n      refreshData();\n    });\n    const __returned__ = {\n      monitorStats,\n      selectedPoint,\n      monitorPoints,\n      realtimeData,\n      isChartReady,\n      getChartOptions,\n      trendTimeRange,\n      trendChartOptions,\n      searchKeyword,\n      filteredMonitorList,\n      alerts,\n      getStatusIcon,\n      getStatusType,\n      getTrendIcon,\n      handleChartError,\n      refreshData,\n      showSettings,\n      selectMonitorPoint,\n      get updateInterval() {\n        return updateInterval;\n      },\n      set updateInterval(v) {\n        updateInterval = v;\n      },\n      ref,\n      computed,\n      onMounted,\n      onBeforeUnmount,\n      watch,\n      nextTick,\n      BaseEChart,\n      get DataLine() {\n        return DataLine;\n      },\n      get Warning() {\n        return Warning;\n      },\n      get Location() {\n        return Location;\n      },\n      get Check() {\n        return Check;\n      },\n      get ArrowUp() {\n        return ArrowUp;\n      },\n      get ArrowDown() {\n        return ArrowDown;\n      },\n      get Setting() {\n        return Setting;\n      },\n      get Refresh() {\n        return Refresh;\n      },\n      get Search() {\n        return Search;\n      },\n      get CircleClose() {\n        return CircleClose;\n      },\n      get echarts() {\n        return echarts;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","onBeforeUnmount","watch","nextTick","BaseEChart","DataLine","Warning","Location","Check","ArrowUp","ArrowDown","Setting","Refresh","Search","CircleClose","echarts","monitorStats","value","label","type","icon","trend","format","selectedPoint","monitorPoints","realtimeData","unit","isAbnormal","trendData","isChartReady","getChartOptions","item","animation","grid","top","right","bottom","left","xAxis","show","boundaryGap","yAxis","series","data","smooth","showSymbol","lineStyle","color","areaStyle","graphic","LinearGradient","offset","trendTimeRange","trendChartOptions","tooltip","trigger","axisPointer","legend","containLabel","name","position","itemStyle","yAxisIndex","searchKeyword","filteredMonitorList","keyword","toLowerCase","status","lastUpdate","filter","includes","alerts","id","title","time","location","level","getStatusIcon","icons","getStatusType","types","getTrendIcon","handleChartError","error","console","refreshData","showSettings","selectMonitorPoint","point","updateInterval","setInterval","clearInterval"],"sources":["D:/restore/water-melt/frontend/src/views/Analysis.vue"],"sourcesContent":["<!-- Analysis.vue -->\r\n<template>\r\n  <div class=\"analysis-view\">\r\n    <!-- 顶部统计卡片 -->\r\n    <div class=\"stats-section\">\r\n      <div class=\"stat-card primary\">\r\n        <div class=\"stat-icon\">\r\n          <el-icon><DataLine /></el-icon>\r\n        </div>\r\n        <div class=\"stat-content\">\r\n          <div class=\"stat-value\">{{ monitorStats[0].value }}</div>\r\n          <div class=\"stat-label\">今日监测数据</div>\r\n          <div class=\"stat-trend up\">\r\n            <el-icon><ArrowUp /></el-icon>\r\n            <span>{{ monitorStats[0].trend }}%</span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"stat-card warning\">\r\n        <div class=\"stat-icon\">\r\n          <el-icon><Warning /></el-icon>\r\n        </div>\r\n        <div class=\"stat-content\">\r\n          <div class=\"stat-value\">{{ monitorStats[1].value }}</div>\r\n          <div class=\"stat-label\">异常数据</div>\r\n          <div class=\"stat-trend down\">\r\n            <el-icon><ArrowDown /></el-icon>\r\n            <span>{{ monitorStats[1].trend }}%</span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"stat-card success\">\r\n        <div class=\"stat-icon\">\r\n          <el-icon><Location /></el-icon>\r\n        </div>\r\n        <div class=\"stat-content\">\r\n          <div class=\"stat-value\">{{ monitorStats[2].value }}</div>\r\n          <div class=\"stat-label\">监测点总数</div>\r\n          <div class=\"stat-trend up\">\r\n            <el-icon><ArrowUp /></el-icon>\r\n            <span>{{ monitorStats[2].trend }}</span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"stat-card info\">\r\n        <div class=\"stat-icon\">\r\n          <el-icon><Check /></el-icon>\r\n        </div>\r\n        <div class=\"stat-content\">\r\n          <div class=\"stat-value\">{{ monitorStats[3].value }}%</div>\r\n          <div class=\"stat-label\">正常率</div>\r\n          <div class=\"stat-trend up\">\r\n            <el-icon><ArrowUp /></el-icon>\r\n            <span>{{ monitorStats[3].trend }}%</span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 主要内容区域 -->\r\n    <div class=\"main-content\">\r\n      <!-- 左侧：实时监测数据 -->\r\n      <div class=\"content-main\">\r\n        <div class=\"monitor-panel\">\r\n          <div class=\"panel-header\">\r\n            <div class=\"header-left\">\r\n              <h3>实时监测数据</h3>\r\n              <el-select \r\n                v-model=\"selectedPoint\" \r\n                size=\"small\" \r\n                placeholder=\"选择监测点\"\r\n                class=\"point-select\"\r\n              >\r\n                <el-option \r\n                  v-for=\"point in monitorPoints\" \r\n                  :key=\"point.value\" \r\n                  :label=\"point.label\" \r\n                  :value=\"point.value\" \r\n                />\r\n              </el-select>\r\n            </div>\r\n            <div class=\"header-right\">\r\n              <el-button-group>\r\n                <el-button size=\"small\" type=\"primary\" @click=\"refreshData\">\r\n                  <el-icon><Refresh /></el-icon>\r\n                </el-button>\r\n                <el-button size=\"small\" @click=\"showSettings\">\r\n                  <el-icon><Setting /></el-icon>\r\n                </el-button>\r\n              </el-button-group>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"monitor-grid\">\r\n            <div \r\n              v-for=\"(item, index) in realtimeData\" \r\n              :key=\"index\"\r\n              :class=\"['monitor-card', { 'warning': item.isAbnormal }]\"\r\n            >\r\n              <div class=\"card-header\">\r\n                <span class=\"card-title\">{{ item.label }}</span>\r\n                <el-tag \r\n                  :type=\"item.isAbnormal ? 'danger' : 'success'\"\r\n                  size=\"small\"\r\n                >\r\n                  {{ item.trend }}\r\n                </el-tag>\r\n              </div>\r\n              <div class=\"card-value\">\r\n                {{ item.value }}\r\n                <span class=\"unit\">{{ item.unit }}</span>\r\n              </div>\r\n              <div class=\"card-chart\">\r\n                <base-e-chart \r\n                  v-if=\"isChartReady\"\r\n                  :options=\"getChartOptions(item)\" \r\n                  height=\"60px\"\r\n                  min-height=\"60px\"\r\n                  :auto-resize=\"true\"\r\n                  @chart-error=\"handleChartError\"\r\n                />\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 趋势分析图表 -->\r\n        <div class=\"trend-panel\">\r\n          <div class=\"panel-header\">\r\n            <h3>趋势分析</h3>\r\n            <el-radio-group v-model=\"trendTimeRange\" size=\"small\">\r\n              <el-radio-button label=\"day\">24小时</el-radio-button>\r\n              <el-radio-button label=\"week\">7天</el-radio-button>\r\n              <el-radio-button label=\"month\">30天</el-radio-button>\r\n            </el-radio-group>\r\n          </div>\r\n          <div class=\"trend-chart\">\r\n            <base-e-chart \r\n              v-if=\"isChartReady\"\r\n              :options=\"trendChartOptions\" \r\n              height=\"300px\"\r\n              min-height=\"300px\"\r\n              :auto-resize=\"true\"\r\n              @chart-error=\"handleChartError\"\r\n            />\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 右侧：监测点列表和告警信息 -->\r\n      <div class=\"content-side\">\r\n        <!-- 监测点列表 -->\r\n        <div class=\"side-panel\">\r\n          <div class=\"panel-header\">\r\n            <h3>监测点状态</h3>\r\n            <el-input\r\n              v-model=\"searchKeyword\"\r\n              size=\"small\"\r\n              placeholder=\"搜索监测点\"\r\n              prefix-icon=\"Search\"\r\n            />\r\n          </div>\r\n          <div class=\"monitor-list\">\r\n            <div \r\n              v-for=\"item in filteredMonitorList\" \r\n              :key=\"item.name\"\r\n              :class=\"['monitor-item', item.status.toLowerCase()]\"\r\n              @click=\"selectMonitorPoint(item)\"\r\n            >\r\n              <div class=\"item-icon\">\r\n                <el-icon><component :is=\"getStatusIcon(item.status)\" /></el-icon>\r\n              </div>\r\n              <div class=\"item-content\">\r\n                <div class=\"item-header\">\r\n                  <span class=\"item-name\">{{ item.name }}</span>\r\n                  <el-tag \r\n                    :type=\"getStatusType(item.status)\" \r\n                    size=\"small\"\r\n                  >\r\n                    {{ item.status }}\r\n                  </el-tag>\r\n                </div>\r\n                <div class=\"item-info\">\r\n                  <span class=\"item-type\">{{ item.type }}</span>\r\n                  <span class=\"item-time\">{{ item.lastUpdate }}</span>\r\n                </div>\r\n              </div>\r\n              <div class=\"item-trend\">\r\n                <el-icon :class=\"getTrendIcon(item.trend)\">\r\n                  <component :is=\"getTrendIcon(item.trend)\" />\r\n                </el-icon>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 告警信息 -->\r\n        <div class=\"side-panel\">\r\n          <div class=\"panel-header\">\r\n            <h3>告警信息</h3>\r\n          </div>\r\n          <div class=\"alarm-list\">\r\n            <el-empty v-if=\"!alerts.length\" description=\"暂无告警\" />\r\n            <div \r\n              v-for=\"alert in alerts\" \r\n              :key=\"alert.id\"\r\n              :class=\"['alarm-item', alert.level]\"\r\n            >\r\n              <div class=\"alarm-icon\">\r\n                <el-icon><Warning /></el-icon>\r\n              </div>\r\n              <div class=\"alarm-content\">\r\n                <div class=\"alarm-title\">{{ alert.title }}</div>\r\n                <div class=\"alarm-info\">\r\n                  <span class=\"alarm-location\">{{ alert.location }}</span>\r\n                  <span class=\"alarm-time\">{{ alert.time }}</span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, computed, onMounted, onBeforeUnmount, watch, nextTick } from 'vue'\r\nimport BaseEChart from '@/components/BaseEChart.vue'\r\nimport { \r\n  DataLine, Warning, Location, Check, ArrowUp, ArrowDown,\r\n  Setting, Refresh, Search, CircleClose \r\n} from '@element-plus/icons-vue'\r\nimport * as echarts from 'echarts'\r\n\r\n// 监测统计数据\r\nconst monitorStats = ref([\r\n  {\r\n    value: 256,\r\n    label: '今日监测数据',\r\n    type: 'primary',\r\n    icon: DataLine,\r\n    trend: 12,\r\n    format: '0,0'\r\n  },\r\n  {\r\n    value: 5,\r\n    label: '异常数据',\r\n    type: 'warning',\r\n    icon: Warning,\r\n    trend: -5,\r\n    format: '0,0'\r\n  },\r\n  {\r\n    value: 32,\r\n    label: '监测点总数',\r\n    type: 'info',\r\n    icon: Location,\r\n    trend: 2,\r\n    format: '0,0'\r\n  },\r\n  {\r\n    value: 98,\r\n    label: '正常率',\r\n    type: 'success',\r\n    icon: Check,\r\n    trend: 3,\r\n    format: '0.00'\r\n  }\r\n])\r\n\r\n// 监测点选择\r\nconst selectedPoint = ref('point1')\r\nconst monitorPoints = [\r\n  { label: '监测点A', value: 'point1' },\r\n  { label: '监测点B', value: 'point2' },\r\n  { label: '监测点C', value: 'point3' }\r\n]\r\n\r\n// 实时数据\r\nconst realtimeData = ref([\r\n  { \r\n    label: '温度', \r\n    value: 25.6,\r\n    unit: '°C',\r\n    icon: Location, \r\n    isAbnormal: false,\r\n    trend: '上升',\r\n    trendData: [23.1, 24.2, 24.8, 25.1, 25.6]\r\n  },\r\n  { \r\n    label: '湿度', \r\n    value: 65.2,\r\n    unit: '%',\r\n    icon: Warning, \r\n    isAbnormal: true,\r\n    trend: '稳定',\r\n    trendData: [65.0, 65.1, 65.2, 65.2, 65.2]\r\n  },\r\n  { \r\n    label: 'PH值', \r\n    value: 6.8,\r\n    unit: '',\r\n    icon: Check, \r\n    isAbnormal: false,\r\n    trend: '下降',\r\n    trendData: [7.2, 7.0, 6.9, 6.8, 6.8]\r\n  }\r\n])\r\n\r\n// 图表加载状态\r\nconst isChartReady = ref(false)\r\n\r\n// 获取小图表配置\r\nconst getChartOptions = (item) => ({\r\n  animation: false, // 禁用动画以提高性能\r\n  grid: {\r\n    top: 0,\r\n    right: 0,\r\n    bottom: 0,\r\n    left: 0\r\n  },\r\n  xAxis: {\r\n    type: 'category',\r\n    show: false,\r\n    boundaryGap: false\r\n  },\r\n  yAxis: {\r\n    type: 'value',\r\n    show: false\r\n  },\r\n  series: [{\r\n    data: item.trendData,\r\n    type: 'line',\r\n    smooth: true,\r\n    showSymbol: false,\r\n    lineStyle: {\r\n      color: item.isAbnormal ? '#f56c6c' : '#67c23a'\r\n    },\r\n    areaStyle: {\r\n      color: new echarts.graphic.LinearGradient(0, 0, 0, 1, [{\r\n        offset: 0,\r\n        color: item.isAbnormal ? 'rgba(245, 108, 108, 0.2)' : 'rgba(103, 194, 58, 0.2)'\r\n      }, {\r\n        offset: 1,\r\n        color: 'rgba(255, 255, 255, 0)'\r\n      }])\r\n    }\r\n  }]\r\n})\r\n\r\n// 趋势分析\r\nconst trendTimeRange = ref('day')\r\nconst trendChartOptions = computed(() => ({\r\n  animation: false, // 禁用动画以提高性能\r\n  tooltip: {\r\n    trigger: 'axis',\r\n    axisPointer: {\r\n      type: 'cross'\r\n    }\r\n  },\r\n  legend: {\r\n    data: ['温度', '湿度', 'PH值']\r\n  },\r\n  grid: {\r\n    left: '3%',\r\n    right: '4%',\r\n    bottom: '3%',\r\n    containLabel: true\r\n  },\r\n  xAxis: {\r\n    type: 'category',\r\n    boundaryGap: false,\r\n    data: ['00:00', '06:00', '12:00', '18:00', '24:00']\r\n  },\r\n  yAxis: [\r\n    {\r\n      type: 'value',\r\n      name: '温度(°C)/湿度(%)',\r\n      position: 'left'\r\n    },\r\n    {\r\n      type: 'value',\r\n      name: 'PH值',\r\n      position: 'right'\r\n    }\r\n  ],\r\n  series: [\r\n    {\r\n      name: '温度',\r\n      type: 'line',\r\n      smooth: true,\r\n      data: [22, 23, 25, 26, 25],\r\n      itemStyle: {\r\n        color: '#409eff'\r\n      }\r\n    },\r\n    {\r\n      name: '湿度',\r\n      type: 'line',\r\n      smooth: true,\r\n      data: [65, 64, 65, 66, 65],\r\n      itemStyle: {\r\n        color: '#67c23a'\r\n      }\r\n    },\r\n    {\r\n      name: 'PH值',\r\n      type: 'line',\r\n      smooth: true,\r\n  