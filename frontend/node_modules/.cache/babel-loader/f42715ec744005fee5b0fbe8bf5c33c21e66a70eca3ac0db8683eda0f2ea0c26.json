{"ast":null,"code":"import BaseEChart from '@/components/BaseEChart.vue';\nimport DataCard from '@/components/DataCard.vue';\nimport { ref, onMounted, onBeforeUnmount } from 'vue';\nimport * as echarts from 'echarts';\nimport chinaMapData from '@/assets/china.json';\nexport default {\n  name: 'SystemOverview',\n  components: {\n    BaseEChart,\n    DataCard\n  },\n  setup() {\n    echarts.registerMap('china', chinaMapData);\n    const selectedYear = ref('2021');\n    const years = ['2021', '2020', '2019', '2018'];\n    const currentTime = ref(new Date().toLocaleString());\n    const timer = ref(null);\n    const currentImage = ref({\n      id: '167599247012556843',\n      satellite: 'LANDSAT 8',\n      longitude: '107.50233-110.08263',\n      latitude: '33.50790-35.69403'\n    });\n    const statisticsData = ref({\n      totalPoints: 2547,\n      todayNewData: 156,\n      abnormalPoints: 18,\n      severeAreas: 3\n    });\n    const alerts = ref([{\n      title: '监测点A12水质超标',\n      time: '10分钟前',\n      level: 'severe'\n    }, {\n      title: '监测点B23土壤重金属含量异常',\n      time: '30分钟前',\n      level: 'moderate'\n    }, {\n      title: '监测点C45数据传输异常',\n      time: '1小时前',\n      level: 'light'\n    }]);\n\n    // 中国地图配置\n    const mapOptions = ref({\n      backgroundColor: 'rgba(0,0,0,0)',\n      title: {\n        text: '全国土壤监测点分布',\n        left: 'center',\n        textStyle: {\n          color: '#fff'\n        }\n      },\n      tooltip: {\n        trigger: 'item',\n        formatter: '{b}: {c} (监测点数量)'\n      },\n      visualMap: {\n        min: 0,\n        max: 200,\n        left: 'left',\n        top: 'bottom',\n        text: ['高', '低'],\n        textStyle: {\n          color: '#fff'\n        },\n        inRange: {\n          color: ['#52c41a', '#fadb14', '#faad14', '#ff4d4f']\n        },\n        calculable: true\n      },\n      series: [{\n        name: '监测点数量',\n        type: 'map',\n        map: 'china',\n        roam: true,\n        emphasis: {\n          label: {\n            show: true\n          }\n        },\n        data: [{\n          name: '北京',\n          value: 98\n        }, {\n          name: '天津',\n          value: 76\n        }, {\n          name: '上海',\n          value: 110\n        }, {\n          name: '重庆',\n          value: 85\n        }, {\n          name: '河北',\n          value: 123\n        }, {\n          name: '河南',\n          value: 145\n        }, {\n          name: '云南',\n          value: 76\n        }, {\n          name: '辽宁',\n          value: 89\n        }, {\n          name: '黑龙江',\n          value: 65\n        }, {\n          name: '湖南',\n          value: 132\n        }, {\n          name: '安徽',\n          value: 96\n        }, {\n          name: '山东',\n          value: 165\n        }, {\n          name: '新疆',\n          value: 45\n        }, {\n          name: '江苏',\n          value: 158\n        }, {\n          name: '浙江',\n          value: 142\n        }, {\n          name: '江西',\n          value: 94\n        }, {\n          name: '湖北',\n          value: 116\n        }, {\n          name: '广西',\n          value: 78\n        }, {\n          name: '甘肃',\n          value: 52\n        }, {\n          name: '山西',\n          value: 84\n        }, {\n          name: '内蒙古',\n          value: 58\n        }, {\n          name: '陕西',\n          value: 79\n        }, {\n          name: '吉林',\n          value: 62\n        }, {\n          name: '福建',\n          value: 89\n        }, {\n          name: '贵州',\n          value: 71\n        }, {\n          name: '广东',\n          value: 168\n        }, {\n          name: '青海',\n          value: 34\n        }, {\n          name: '西藏',\n          value: 29\n        }, {\n          name: '四川',\n          value: 124\n        }, {\n          name: '宁夏',\n          value: 38\n        }, {\n          name: '海南',\n          value: 45\n        }, {\n          name: '台湾',\n          value: 32\n        }, {\n          name: '香港',\n          value: 28\n        }, {\n          name: '澳门',\n          value: 16\n        }]\n      }]\n    });\n\n    // 更新时间\n    const updateTime = () => {\n      currentTime.value = new Date().toLocaleString();\n    };\n    onMounted(() => {\n      // 初始化加载数据\n      timer.value = setInterval(updateTime, 1000);\n    });\n    onBeforeUnmount(() => {\n      if (timer.value) {\n        clearInterval(timer.value);\n      }\n    });\n    return {\n      selectedYear,\n      years,\n      currentImage,\n      mapOptions,\n      currentTime,\n      statisticsData,\n      alerts\n    };\n  }\n};","map":{"version":3,"names":["BaseEChart","DataCard","ref","onMounted","onBeforeUnmount","echarts","chinaMapData","name","components","setup","registerMap","selectedYear","years","currentTime","Date","toLocaleString","timer","currentImage","id","satellite","longitude","latitude","statisticsData","totalPoints","todayNewData","abnormalPoints","severeAreas","alerts","title","time","level","mapOptions","backgroundColor","text","left","textStyle","color","tooltip","trigger","formatter","visualMap","min","max","top","inRange","calculable","series","type","map","roam","emphasis","label","show","data","value","updateTime","setInterval","clearInterval"],"sources":["D:\\restore\\water-melt\\frontend\\src\\views\\SystemOverview.vue"],"sourcesContent":["<!-- SystemOverview.vue -->\r\n<template>\r\n  <div class=\"system-overview\">\r\n    <div class=\"overview-header\">\r\n      <h2>系统概览</h2>\r\n      <div class=\"time-info\">\r\n        <i class=\"el-icon-time\"></i>\r\n        <span>{{ currentTime }}</span>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"data-cards\">\r\n      <data-card \r\n        title=\"监测点总数\" \r\n        :value=\"statisticsData.totalPoints\" \r\n        icon=\"Location\" \r\n        color=\"#1890ff\"\r\n      />\r\n      <data-card \r\n        title=\"今日新增数据\" \r\n        :value=\"statisticsData.todayNewData\" \r\n        icon=\"DataLine\" \r\n        color=\"#52c41a\"\r\n      />\r\n      <data-card \r\n        title=\"异常监测点\" \r\n        :value=\"statisticsData.abnormalPoints\" \r\n        icon=\"Warning\" \r\n        color=\"#faad14\"\r\n      />\r\n      <data-card \r\n        title=\"严重污染区域\" \r\n        :value=\"statisticsData.severeAreas\" \r\n        icon=\"CircleClose\" \r\n        color=\"#ff4d4f\"\r\n      />\r\n    </div>\r\n\r\n    <div class=\"main-content\">\r\n      <!-- 土壤污染指数图例 -->\r\n      <div class=\"pollution-legend\">\r\n        <h3>土壤污染指数</h3>\r\n        <div class=\"legend-items\">\r\n          <div class=\"legend-item\">\r\n            <div class=\"color-block severe\"></div>\r\n            <span>严重污染 > 80</span>\r\n          </div>\r\n          <div class=\"legend-item\">\r\n            <div class=\"color-block moderate\"></div>\r\n            <span>中度污染 60-80</span>\r\n          </div>\r\n          <div class=\"legend-item\">\r\n            <div class=\"color-block light\"></div>\r\n            <span>轻度污染 40-60</span>\r\n          </div>\r\n          <div class=\"legend-item\">\r\n            <div class=\"color-block good\"></div>\r\n            <span>良好 < 40</span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 地图容器 -->\r\n      <div class=\"map-container\">\r\n        <base-e-chart :options=\"mapOptions\" />\r\n      </div>\r\n\r\n      <!-- 右侧信息面板 -->\r\n      <div class=\"info-panel\">\r\n        <div class=\"image-info\">\r\n          <h3>影像时段年份：</h3>\r\n          <el-select v-model=\"selectedYear\" placeholder=\"选择年份\">\r\n            <el-option v-for=\"year in years\" :key=\"year\" :label=\"year\" :value=\"year\" />\r\n          </el-select>\r\n        </div>\r\n        <div class=\"image-details\">\r\n          <h3>影像信息：</h3>\r\n          <div class=\"detail-item\">\r\n            <span class=\"label\">唯一编号：</span>\r\n            <span class=\"value\">{{ currentImage.id }}</span>\r\n          </div>\r\n          <div class=\"detail-item\">\r\n            <span class=\"label\">卫星类型：</span>\r\n            <span class=\"value\">{{ currentImage.satellite }}</span>\r\n          </div>\r\n          <div class=\"detail-item\">\r\n            <span class=\"label\">经度范围：</span>\r\n            <span class=\"value\">{{ currentImage.longitude }}</span>\r\n          </div>\r\n          <div class=\"detail-item\">\r\n            <span class=\"label\">纬度范围：</span>\r\n            <span class=\"value\">{{ currentImage.latitude }}</span>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"recent-alerts\">\r\n          <h3>最近告警：</h3>\r\n          <div class=\"alert-list\">\r\n            <div v-for=\"(alert, index) in alerts\" :key=\"index\" class=\"alert-item\" :class=\"alert.level\">\r\n              <div class=\"alert-icon\">\r\n                <el-icon><Warning /></el-icon>\r\n              </div>\r\n              <div class=\"alert-content\">\r\n                <div class=\"alert-title\">{{ alert.title }}</div>\r\n                <div class=\"alert-time\">{{ alert.time }}</div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"tif-preview-section\">\r\n      <h3>最新12张遥感影像预览（.tif）</h3>\r\n      <div class=\"tif-preview-grid\">\r\n        <div v-for=\"(tif, idx) in tifFiles\" :key=\"tif.name\" class=\"tif-preview-item\">\r\n          <canvas :ref=\"el => tifCanvases[idx] = el\" width=\"160\" height=\"120\"></canvas>\r\n          <div class=\"tif-caption\">{{ tif.name }}</div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport BaseEChart from '@/components/BaseEChart.vue'\r\nimport DataCard from '@/components/DataCard.vue'\r\nimport { ref, onMounted, onBeforeUnmount } from 'vue'\r\nimport * as echarts from 'echarts'\r\nimport chinaMapData from '@/assets/china.json'\r\n\r\nexport default {\r\n  name: 'SystemOverview',\r\n  components: {\r\n    BaseEChart,\r\n    DataCard\r\n  },\r\n  setup() {\r\n    echarts.registerMap('china', chinaMapData)\r\n    \r\n    const selectedYear = ref('2021')\r\n    const years = ['2021', '2020', '2019', '2018']\r\n    const currentTime = ref(new Date().toLocaleString())\r\n    const timer = ref(null)\r\n    \r\n    const currentImage = ref({\r\n      id: '167599247012556843',\r\n      satellite: 'LANDSAT 8',\r\n      longitude: '107.50233-110.08263',\r\n      latitude: '33.50790-35.69403'\r\n    })\r\n\r\n    const statisticsData = ref({\r\n      totalPoints: 2547,\r\n      todayNewData: 156,\r\n      abnormalPoints: 18,\r\n      severeAreas: 3\r\n    })\r\n\r\n    const alerts = ref([\r\n      { \r\n        title: '监测点A12水质超标', \r\n        time: '10分钟前', \r\n        level: 'severe' \r\n      },\r\n      { \r\n        title: '监测点B23土壤重金属含量异常', \r\n        time: '30分钟前', \r\n        level: 'moderate' \r\n      },\r\n      { \r\n        title: '监测点C45数据传输异常', \r\n        time: '1小时前', \r\n        level: 'light' \r\n      }\r\n    ])\r\n\r\n    // 中国地图配置\r\n    const mapOptions = ref({\r\n      backgroundColor: 'rgba(0,0,0,0)',\r\n      title: {\r\n        text: '全国土壤监测点分布',\r\n        left: 'center',\r\n        textStyle: {\r\n          color: '#fff'\r\n        }\r\n      },\r\n      tooltip: {\r\n        trigger: 'item',\r\n        formatter: '{b}: {c} (监测点数量)'\r\n      },\r\n      visualMap: {\r\n        min: 0,\r\n        max: 200,\r\n        left: 'left',\r\n        top: 'bottom',\r\n        text: ['高', '低'],\r\n        textStyle: {\r\n          color: '#fff'\r\n        },\r\n        inRange: {\r\n          color: ['#52c41a', '#fadb14', '#faad14', '#ff4d4f']\r\n        },\r\n        calculable: true\r\n      },\r\n      series: [\r\n        {\r\n          name: '监测点数量',\r\n          type: 'map',\r\n          map: 'china',\r\n          roam: true,\r\n          emphasis: {\r\n            label: {\r\n              show: true\r\n            }\r\n          },\r\n          data: [\r\n            { name: '北京', value: 98 },\r\n            { name: '天津', value: 76 },\r\n            { name: '上海', value: 110 },\r\n            { name: '重庆', value: 85 },\r\n            { name: '河北', value: 123 },\r\n            { name: '河南', value: 145 },\r\n            { name: '云南', value: 76 },\r\n            { name: '辽宁', value: 89 },\r\n            { name: '黑龙江', value: 65 },\r\n            { name: '湖南', value: 132 },\r\n            { name: '安徽', value: 96 },\r\n            { name: '山东', value: 165 },\r\n            { name: '新疆', value: 45 },\r\n            { name: '江苏', value: 158 },\r\n            { name: '浙江', value: 142 },\r\n            { name: '江西', value: 94 },\r\n            { name: '湖北', value: 116 },\r\n            { name: '广西', value: 78 },\r\n            { name: '甘肃', value: 52 },\r\n            { name: '山西', value: 84 },\r\n            { name: '内蒙古', value: 58 },\r\n            { name: '陕西', value: 79 },\r\n            { name: '吉林', value: 62 },\r\n            { name: '福建', value: 89 },\r\n            { name: '贵州', value: 71 },\r\n            { name: '广东', value: 168 },\r\n            { name: '青海', value: 34 },\r\n            { name: '西藏', value: 29 },\r\n            { name: '四川', value: 124 },\r\n            { name: '宁夏', value: 38 },\r\n            { name: '海南', value: 45 },\r\n            { name: '台湾', value: 32 },\r\n            { name: '香港', value: 28 },\r\n            { name: '澳门', value: 16 }\r\n          ]\r\n        }\r\n      ]\r\n    })\r\n\r\n    // 更新时间\r\n    const updateTime = () => {\r\n      currentTime.value = new Date().toLocaleString()\r\n    }\r\n\r\n    onMounted(() => {\r\n      // 初始化加载数据\r\n      timer.value = setInterval(updateTime, 1000)\r\n    })\r\n\r\n    onBeforeUnmount(() => {\r\n      if (timer.value) {\r\n        clearInterval(timer.value)\r\n      }\r\n    })\r\n\r\n    return {\r\n      selectedYear,\r\n      years,\r\n      currentImage,\r\n      mapOptions,\r\n      currentTime,\r\n      statisticsData,\r\n      alerts\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.system