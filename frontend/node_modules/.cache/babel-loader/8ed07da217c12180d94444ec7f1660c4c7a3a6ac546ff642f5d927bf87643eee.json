","years","chartOptions","tooltip","trigger","axisPointer","type","legend","data","textStyle","color","grid","left","right","bottom","containLabel","xAxis","axisLine","lineStyle","axisLabel","yAxis","nameTextStyle","splitLine","series","itemStyle","Function","startEvaluation"],"sources":["D:\\restore\\water-melt\\frontend\\src\\views\\QualityEvaluation.vue"],"sourcesContent":["<!-- QualityEvaluation.vue -->\r\n<template>\r\n  <div class=\"quality-evaluation\">\r\n    <!-- 评价指标面板 -->\r\n    <div class=\"evaluation-panel\">\r\n      <h3>综合评价</h3>\r\n      <div class=\"indicator-grid\">\r\n        <div class=\"indicator-group\">\r\n          <h4>地效评价</h4>\r\n          <div class=\"indicators\">\r\n            <div class=\"indicator\">\r\n              <span class=\"label\">全量</span>\r\n              <div class=\"value\">85</div>\r\n            </div>\r\n            <div class=\"indicator\">\r\n              <span class=\"label\">有机质</span>\r\n              <div class=\"value\">78</div>\r\n            </div>\r\n            <div class=\"indicator\">\r\n              <span class=\"label\">碱解氮</span>\r\n              <div class=\"value\">92</div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"indicator-group\">\r\n          <h4>土壤评价</h4>\r\n          <div class=\"indicators\">\r\n            <div class=\"indicator\">\r\n              <span class=\"label\">土壤肥力</span>\r\n              <div class=\"value\">76</div>\r\n            </div>\r\n            <div class=\"indicator\">\r\n              <span class=\"label\">土壤质量</span>\r\n              <div class=\"value\">88</div>\r\n            </div>\r\n            <div class=\"indicator\">\r\n              <span class=\"label\">土壤侵蚀</span>\r\n              <div class=\"value\">65</div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 图表区域 -->\r\n    <div class=\"chart-container\">\r\n      <div class=\"chart-header\">\r\n        <h3>各等级农田面积占比</h3>\r\n        <div class=\"chart-controls\">\r\n          <el-select v-model=\"selectedYear\" placeholder=\"选择年份\">\r\n            <el-option\r\n              v-for=\"year in years\"\r\n              :key=\"year\"\r\n              :label=\"year\"\r\n              :value=\"year\"\r\n            />\r\n          </el-select>\r\n        </div>\r\n      </div>\r\n      <div class=\"chart-content\">\r\n        <base-e-chart :options=\"chartOptions\" />\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 评价结果 -->\r\n    <div class=\"evaluation-result\">\r\n      <h3>综合评价结果</h3>\r\n      <div class=\"result-content\">\r\n        <p>当前智能土壤环境综合评价，请先进行综合评价。</p>\r\n        <el-button type=\"primary\" @click=\"startEvaluation\">\r\n          开始进行综合评价\r\n        </el-button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, reactive } from 'vue'\r\nimport BaseEChart from '@/components/BaseEChart.vue'\r\n\r\nexport default {\r\n  name: 'QualityEvaluation',\r\n  components: {\r\n    BaseEChart\r\n  },\r\n  setup() {\r\n    const selectedYear = ref('2021')\r\n    const years = ['2021', '2020', '2019', '2018']\r\n\r\n    const chartOptions = reactive({\r\n      tooltip: {\r\n        trigger: 'axis',\r\n        axisPointer: {\r\n          type: 'shadow'\r\n        }\r\n      },\r\n      legend: {\r\n        data: ['面积占比'],\r\n        textStyle: {\r\n          color: '#fff'\r\n        }\r\n      },\r\n      grid: {\r\n        left: '3%',\r\n        right: '4%',\r\n        bottom: '3%',\r\n        containLabel: true\r\n      },\r\n      xAxis: {\r\n        type: 'category',\r\n        data: ['未监测', '一等级', '二等级', '三等级', '四等级', '五等级'],\r\n        axisLine: {\r\n          lineStyle: {\r\n            color: '#fff'\r\n          }\r\n        },\r\n        axisLabel: {\r\n          color: '#fff'\r\n        }\r\n      },\r\n      yAxis: {\r\n        type: 'value',\r\n        name: '面积占比（公顷）',\r\n        nameTextStyle: {\r\n          color: '#fff'\r\n        },\r\n        axisLine: {\r\n          lineStyle: {\r\n            color: '#fff'\r\n          }\r\n        },\r\n        axisLabel: {\r\n          color: '#fff'\r\n        },\r\n        splitLine: {\r\n          lineStyle: {\r\n            color: 'rgba(255, 255, 255, 0.1)'\r\n          }\r\n        }\r\n      },\r\n      series: [\r\n        {\r\n          name: '面积占比',\r\n          type: 'bar',\r\n          data: [10, 25, 30, 20, 10, 5],\r\n          itemStyle: {\r\n            color: new Function('params', `\r\n              const colors = ['#666666', '#2f54eb', '#52c41a', '#faad14', '#ff4d4f', '#722ed1'];\r\n              return colors[params.dataIndex];\r\n            `)\r\n          }\r\n        }\r\n      ]\r\n    })\r\n\r\n    const startEvaluation = () => {\r\n      // 实现开始评价的逻辑\r\n    }\r\n\r\n    return {\r\n      selectedYear,\r\n      years,\r\n      chartOptions,\r\n      startEvaluation\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.quality-evaluation {\r\n  padding: 20px;\r\n  display: grid;\r\n  grid-template-columns: 1fr 1fr;\r\n  grid-template-rows: auto 1fr;\r\n  gap: 20px;\r\n  height: calc(100vh - 80px);\r\n  background-color: #001529;\r\n  color: #fff;\r\n}\r\n\r\n.evaluation-panel {\r\n  grid-column: 1 / -1;\r\n  background: rgba(255, 255, 255, 0.04);\r\n  border-radius: 4px;\r\n  padding: 20px;\r\n\r\n  h3 {\r\n    margin: 0 0 20px;\r\n    font-size: 18px;\r\n  }\r\n\r\n  .indicator-grid {\r\n    display: grid;\r\n    grid-template-columns: repeat(2, 1fr);\r\n    gap: 20px;\r\n  }\r\n\r\n  .indicator-group {\r\n    h4 {\r\n      margin: 0 0 15px;\r\n      color: #8c8c8c;\r\n    }\r\n\r\n    .indicators {\r\n      display: grid;\r\n      grid-template-columns: repeat(3, 1fr);\r\n      gap: 15px;\r\n    }\r\n\r\n    .indicator {\r\n      background: rgba(0, 0, 0, 0.2);\r\n      padding: 15px;\r\n      border-radius: 4px;\r\n      text-align: center;\r\n\r\n      .label {\r\n        color: #8c8c8c;\r\n        font-size: 14px;\r\n      }\r\n\r\n      .value {\r\n        font-size: 24px;\r\n        font-weight: bold;\r\n        margin-top: 10px;\r\n        color: #1890ff;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.chart-container {\r\n  background: rgba(255, 255, 255, 0.04);\r\n  border-radius: 4px;\r\n  padding: 20px;\r\n  display: flex;\r\n  flex-direction: column;\r\n\r\n  .chart-header {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    margin-bottom: 20px;\r\n\r\n    h3 {\r\n      margin: 0;\r\n      font-size: 18px;\r\n    }\r\n  }\r\n\r\n  .chart-content {\r\n    flex: 1;\r\n    min-height: 300px;\r\n  }\r\n}\r\n\r\n.evaluation-result {\r\n  background: rgba(255, 255, 255, 0.04);\r\n  border-radius: 4px;\r\n  padding: 20px;\r\n\r\n  h3 {\r\n    margin: 0 0 20px;\r\n    font-size: 18px;\r\n  }\r\n\r\n  .result-content {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    justify-content: center;\r\n    height: calc(100% - 50px);\r\n    gap: 20px;\r\n    text-align: center;\r\n\r\n    p {\r\n      color: #8c8c8c;\r\n    }\r\n  }\r\n}\r\n</style> "],"mappings":"AA8EA,SAASA,GAAG,EAAEC,QAAO,QAAS,KAAI;AAClC,OAAOC,UAAS,MAAO,6BAA4B;AAEnD,eAAe;EACbC,IAAI,EAAE,mBAAmB;EACzBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,KAAKA,CAAA,EAAG;IACN,MAAMC,YAAW,GAAIN,GAAG,CAAC,MAAM;IAC/B,MAAMO,KAAI,GAAI,CAAC,MAAM,EAAE,MAAM,EAAE,MAAM,EAAE,MAAM;IAE7C,MAAMC,YAAW,GAAIP,QAAQ,CAAC;MAC5BQ,OAAO,EAAE;QACPC,OAAO,EAAE,MAAM;QACfC,WAAW,EAAE;UACXC,IAAI,EAAE;QACR;MACF,CAAC;MACDC,MAAM,EAAE;QACNC,IAAI,EAAE,CAAC,MAAM,CAAC;QACdC,SAAS,EAAE;UACTC,KAAK,EAAE;QACT;MACF,CAAC;MACDC,IAAI,EAAE;QACJC,IAAI,EAAE,IAAI;QACVC,KAAK,EAAE,IAAI;QACXC,MAAM,EAAE,IAAI;QACZC,YAAY,EAAE;MAChB,CAAC;MACDC,KAAK,EAAE;QACLV,IAAI,EAAE,UAAU;QAChBE,IAAI,EAAE,CAAC,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,CAAC;QAChDS,QAAQ,EAAE;UACRC,SAAS,EAAE;YACTR,KAAK,EAAE;UACT;QACF,CAAC;QACDS,SAAS,EAAE;UACTT,KAAK,EAAE;QACT;MACF,CAAC;MACDU,KAAK,EAAE;QACLd,IAAI,EAAE,OAAO;QACbT,IAAI