{"ast":null,"code":"import { ref, computed } from 'vue';\nimport BaseEChart from '@/components/BaseEChart.vue';\nimport { DataLine, Warning, Location, Check, Refresh, Setting, Search, Bell } from '@element-plus/icons-vue';\n\n// 监测统计数据\n\nexport default {\n  __name: 'Analysis',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const monitorStats = ref({\n      todayData: 256,\n      abnormalData: 5,\n      totalPoints: 32,\n      normalRate: 98\n    });\n\n    // 监测点选择\n    const selectedPoint = ref('point1');\n    const monitorPoints = [{\n      label: '监测点A',\n      value: 'point1'\n    }, {\n      label: '监测点B',\n      value: 'point2'\n    }, {\n      label: '监测点C',\n      value: 'point3'\n    }];\n\n    // 实时数据\n    const realtimeData = ref([{\n      label: '土壤温度',\n      value: '23.5°C',\n      icon: 'Thermometer',\n      isAbnormal: false\n    }, {\n      label: '土壤湿度',\n      value: '60%',\n      icon: 'WaterMeter',\n      isAbnormal: false\n    }, {\n      label: 'pH值',\n      value: '7.1',\n      icon: 'Monitor',\n      isAbnormal: true\n    }, {\n      label: '重金属',\n      value: '0.03mg/L',\n      icon: 'DataAnalysis',\n      isAbnormal: false\n    }]);\n\n    // 趋势时间范围\n    const trendTimeRange = ref('day');\n\n    // 趋势图配置\n    const trendChartOptions = computed(() => ({\n      backgroundColor: 'transparent',\n      tooltip: {\n        trigger: 'axis',\n        axisPointer: {\n          type: 'line'\n        }\n      },\n      legend: {\n        data: ['温度', 'pH值', '湿度'],\n        textStyle: {\n          color: '#fff'\n        }\n      },\n      grid: {\n        top: 40,\n        right: 20,\n        bottom: 40,\n        left: 50,\n        containLabel: true\n      },\n      xAxis: {\n        type: 'category',\n        data: ['00:00', '04:00', '08:00', '12:00', '16:00', '20:00', '24:00'],\n        axisLine: {\n          lineStyle: {\n            color: '#8c8c8c'\n          }\n        },\n        axisLabel: {\n          color: '#fff'\n        }\n      },\n      yAxis: {\n        type: 'value',\n        splitLine: {\n          lineStyle: {\n            color: 'rgba(255, 255, 255, 0.1)'\n          }\n        },\n        axisLine: {\n          lineStyle: {\n            color: '#8c8c8c'\n          }\n        },\n        axisLabel: {\n          color: '#fff'\n        }\n      },\n      series: [{\n        name: '温度',\n        type: 'line',\n        smooth: true,\n        data: [23, 24, 23.5, 25, 26, 25, 24],\n        itemStyle: {\n          color: '#1890ff'\n        }\n      }, {\n        name: 'pH值',\n        type: 'line',\n        smooth: true,\n        data: [7.0, 7.1, 7.2, 7.1, 7.0, 7.2, 7.1],\n        itemStyle: {\n          color: '#52c41a'\n        }\n      }, {\n        name: '湿度',\n        type: 'line',\n        smooth: true,\n        data: [60, 62, 65, 63, 61, 64, 62],\n        itemStyle: {\n          color: '#faad14'\n        }\n      }]\n    }));\n\n    // 监测点列表\n    const monitorList = ref([{\n      name: '点A',\n      type: '温度',\n      value: '23.5°C',\n      status: '正常'\n    }, {\n      name: '点B',\n      type: '湿度',\n      value: '60%',\n      status: '正常'\n    }, {\n      name: '点C',\n      type: 'pH值',\n      value: '7.1',\n      status: '异常'\n    }, {\n      name: '点D',\n      type: '重金属',\n      value: '0.03mg/L',\n      status: '正常'\n    }]);\n\n    // 搜索关键词\n    const searchKeyword = ref('');\n\n    // 过滤后的监测点列表\n    const filteredMonitorList = computed(() => {\n      if (!searchKeyword.value) return monitorList.value;\n      const keyword = searchKeyword.value.toLowerCase();\n      return monitorList.value.filter(item => item.name.toLowerCase().includes(keyword) || item.type.toLowerCase().includes(keyword));\n    });\n\n    // 告警信息\n    const alerts = ref([{\n      title: '监测点C pH值异常',\n      location: '区域A-C点',\n      time: '10分钟前',\n      level: 'warning'\n    }, {\n      title: '监测点F数据传输中断',\n      location: '区域B-F点',\n      time: '30分钟前',\n      level: 'error'\n    }, {\n      title: '监测点B湿度波动',\n      location: '区域A-B点',\n      time: '1小时前',\n      level: 'info'\n    }]);\n    const __returned__ = {\n      monitorStats,\n      selectedPoint,\n      monitorPoints,\n      realtimeData,\n      trendTimeRange,\n      trendChartOptions,\n      monitorList,\n      searchKeyword,\n      filteredMonitorList,\n      alerts,\n      ref,\n      computed,\n      BaseEChart,\n      get DataLine() {\n        return DataLine;\n      },\n      get Warning() {\n        return Warning;\n      },\n      get Location() {\n        return Location;\n      },\n      get Check() {\n        return Check;\n      },\n      get Refresh() {\n        return Refresh;\n      },\n      get Setting() {\n        return Setting;\n      },\n      get Search() {\n        return Search;\n      },\n      get Bell() {\n        return Bell;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","BaseEChart","DataLine","Warning","Location","Check","Refresh","Setting","Search","Bell","monitorStats","todayData","abnormalData","totalPoints","normalRate","selectedPoint","monitorPoints","label","value","realtimeData","icon","isAbnormal","trendTimeRange","trendChartOptions","backgroundColor","tooltip","trigger","axisPointer","type","legend","data","textStyle","color","grid","top","right","bottom","left","containLabel","xAxis","axisLine","lineStyle","axisLabel","yAxis","splitLine","series","name","smooth","itemStyle","monitorList","status","searchKeyword","filteredMonitorList","keyword","toLowerCase","filter","item","includes","alerts","title","location","time","level"],"sources":["D:/restore/water-melt/frontend/src/views/Analysis.vue"],"sourcesContent":["<!-- Analysis.vue -->\r\n<template>\r\n  <div class=\"analysis-view\">\r\n    <!-- 顶部统计卡片 -->\r\n    <div class=\"stats-section\">\r\n      <div class=\"stat-card primary\">\r\n        <div class=\"stat-icon\">\r\n          <el-icon><DataLine /></el-icon>\r\n        </div>\r\n        <div class=\"stat-content\">\r\n          <div class=\"stat-value\">{{ monitorStats.todayData }}</div>\r\n          <div class=\"stat-label\">今日监测数据</div>\r\n        </div>\r\n        <div class=\"stat-trend up\">\r\n          <el-icon><ArrowUp /></el-icon>\r\n          <span>12%</span>\r\n        </div>\r\n      </div>\r\n      \r\n      <div class=\"stat-card warning\">\r\n        <div class=\"stat-icon\">\r\n          <el-icon><Warning /></el-icon>\r\n        </div>\r\n        <div class=\"stat-content\">\r\n          <div class=\"stat-value\">{{ monitorStats.abnormalData }}</div>\r\n          <div class=\"stat-label\">异常数据</div>\r\n        </div>\r\n        <div class=\"stat-trend down\">\r\n          <el-icon><ArrowDown /></el-icon>\r\n          <span>5%</span>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"stat-card info\">\r\n        <div class=\"stat-icon\">\r\n          <el-icon><Location /></el-icon>\r\n        </div>\r\n        <div class=\"stat-content\">\r\n          <div class=\"stat-value\">{{ monitorStats.totalPoints }}</div>\r\n          <div class=\"stat-label\">监测点总数</div>\r\n        </div>\r\n        <div class=\"stat-trend up\">\r\n          <el-icon><ArrowUp /></el-icon>\r\n          <span>2个</span>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"stat-card success\">\r\n        <div class=\"stat-icon\">\r\n          <el-icon><Check /></el-icon>\r\n        </div>\r\n        <div class=\"stat-content\">\r\n          <div class=\"stat-value\">{{ monitorStats.normalRate }}%</div>\r\n          <div class=\"stat-label\">正常率</div>\r\n        </div>\r\n        <div class=\"stat-trend up\">\r\n          <el-icon><ArrowUp /></el-icon>\r\n          <span>3%</span>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 主要内容区域 -->\r\n    <div class=\"main-content\">\r\n      <!-- 左侧：实时监测和趋势图表 -->\r\n      <div class=\"content-main\">\r\n        <!-- 实时监测数据 -->\r\n        <div class=\"monitor-panel\">\r\n          <div class=\"panel-header\">\r\n            <h3>实时监测数据</h3>\r\n            <div class=\"panel-controls\">\r\n              <el-select v-model=\"selectedPoint\" size=\"small\" placeholder=\"选择监测点\">\r\n                <el-option\r\n                  v-for=\"point in monitorPoints\"\r\n                  :key=\"point.value\"\r\n                  :label=\"point.label\"\r\n                  :value=\"point.value\"\r\n                />\r\n              </el-select>\r\n              <el-button-group>\r\n                <el-button size=\"small\" type=\"primary\" :icon=\"Refresh\">刷新</el-button>\r\n                <el-button size=\"small\" type=\"primary\" :icon=\"Setting\">设置</el-button>\r\n              </el-button-group>\r\n            </div>\r\n          </div>\r\n          <div class=\"realtime-grid\">\r\n            <div v-for=\"(item, index) in realtimeData\" :key=\"index\" class=\"realtime-item\">\r\n              <div class=\"item-icon\">\r\n                <el-icon><component :is=\"item.icon\" /></el-icon>\r\n              </div>\r\n              <div class=\"item-info\">\r\n                <div class=\"item-label\">{{ item.label }}</div>\r\n                <div class=\"item-value\" :class=\"{ 'warning': item.isAbnormal }\">\r\n                  {{ item.value }}\r\n                  <el-tooltip v-if=\"item.isAbnormal\" effect=\"dark\" content=\"数值异常\" placement=\"top\">\r\n                    <el-icon class=\"warning-icon\"><Warning /></el-icon>\r\n                  </el-tooltip>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 趋势分析图表 -->\r\n        <div class=\"trend-panel\">\r\n          <div class=\"panel-header\">\r\n            <h3>趋势分析</h3>\r\n            <div class=\"panel-controls\">\r\n              <el-radio-group v-model=\"trendTimeRange\" size=\"small\">\r\n                <el-radio-button label=\"day\">24小时</el-radio-button>\r\n                <el-radio-button label=\"week\">7天</el-radio-button>\r\n                <el-radio-button label=\"month\">30天</el-radio-button>\r\n              </el-radio-group>\r\n            </div>\r\n          </div>\r\n          <div class=\"chart-container\">\r\n            <base-e-chart :options=\"trendChartOptions\" height=\"100%\" />\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 右侧边栏：监测列表和告警信息 -->\r\n      <div class=\"content-side\">\r\n        <!-- 监测点列表 -->\r\n        <div class=\"monitor-list-panel\">\r\n          <div class=\"panel-header\">\r\n            <h3>监测点状态</h3>\r\n            <el-input\r\n              v-model=\"searchKeyword\"\r\n              size=\"small\"\r\n              placeholder=\"搜索监测点\"\r\n              prefix-icon=\"Search\"\r\n              style=\"width: 200px\"\r\n            />\r\n          </div>\r\n          <div class=\"monitor-list\">\r\n            <div v-for=\"item in filteredMonitorList\" :key=\"item.name\" class=\"monitor-item\">\r\n              <div class=\"monitor-info\">\r\n                <div class=\"monitor-name\">{{ item.name }}</div>\r\n                <div class=\"monitor-type\">{{ item.type }}</div>\r\n              </div>\r\n              <div class=\"monitor-value\">{{ item.value }}</div>\r\n              <div class=\"monitor-status\" :class=\"item.status.toLowerCase()\">\r\n                {{ item.status }}\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 告警信息 -->\r\n        <div class=\"alert-panel\">\r\n          <div class=\"panel-header\">\r\n            <h3>告警信息</h3>\r\n            <el-button type=\"primary\" size=\"small\" :icon=\"Bell\">全部标记已读</el-button>\r\n          </div>\r\n          <div class=\"alert-list\">\r\n            <div v-for=\"(alert, index) in alerts\" :key=\"index\" class=\"alert-item\" :class=\"alert.level\">\r\n              <div class=\"alert-icon\">\r\n                <el-icon><Warning /></el-icon>\r\n              </div>\r\n              <div class=\"alert-content\">\r\n                <div class=\"alert-title\">{{ alert.title }}</div>\r\n                <div class=\"alert-info\">\r\n                  <span>{{ alert.location }}</span>\r\n                  <span>{{ alert.time }}</span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, computed } from 'vue'\r\nimport BaseEChart from '@/components/BaseEChart.vue'\r\nimport { DataLine, Warning, Location, Check, Refresh, Setting, Search, Bell } from '@element-plus/icons-vue'\r\n\r\n// 监测统计数据\r\nconst monitorStats = ref({\r\n  todayData: 256,\r\n  abnormalData: 5,\r\n  totalPoints: 32,\r\n  normalRate: 98\r\n})\r\n\r\n// 监测点选择\r\nconst selectedPoint = ref('point1')\r\nconst monitorPoints = [\r\n  { label: '监测点A', value: 'point1' },\r\n  { label: '监测点B', value: 'point2' },\r\n  { label: '监测点C', value: 'point3' }\r\n]\r\n\r\n// 实时数据\r\nconst realtimeData = ref([\r\n  { label: '土壤温度', value: '23.5°C', icon: 'Thermometer', isAbnormal: false },\r\n  { label: '土壤湿度', value: '60%', icon: 'WaterMeter', isAbnormal: false },\r\n  { label: 'pH值', value: '7.1', icon: 'Monitor', isAbnormal: true },\r\n  { label: '重金属', value: '0.03mg/L', icon: 'DataAnalysis', isAbnormal: false }\r\n])\r\n\r\n// 趋势时间范围\r\nconst trendTimeRange = ref('day')\r\n\r\n// 趋势图配置\r\nconst trendChartOptions = computed(() => ({\r\n  backgroundColor: 'transparent',\r\n  tooltip: {\r\n    trigger: 'axis',\r\n    axisPointer: {\r\n      type: 'line'\r\n    }\r\n  },\r\n  legend: {\r\n    data: ['温度', 'pH值', '湿度'],\r\n    textStyle: { color: '#fff' }\r\n  },\r\n  grid: {\r\n    top: 40,\r\n    right: 20,\r\n    bottom: 40,\r\n    left: 50,\r\n    containLabel: true\r\n  },\r\n  xAxis: {\r\n    type: 'category',\r\n    data: ['00:00', '04:00', '08:00', '12:00', '16:00', '20:00', '24:00'],\r\n    axisLine: { lineStyle: { color: '#8c8c8c' } },\r\n    axisLabel: { color: '#fff' }\r\n  },\r\n  yAxis: {\r\n    type: 'value',\r\n    splitLine: { lineStyle: { color: 'rgba(255, 255, 255, 0.1)' } },\r\n    axisLine: { lineStyle: { color: '#8c8c8c' } },\r\n    axisLabel: { color: '#fff' }\r\n  },\r\n  series: [\r\n    {\r\n      name: '温度',\r\n      type: 'line',\r\n      smooth: true,\r\n      data: [23, 24, 23.5, 25, 26, 25, 24],\r\n      itemStyle: { color: '#1890ff' }\r\n    },\r\n    {\r\n      name: 'pH值',\r\n      type: 'line',\r\n      smooth: true,\r\n      data: [7.0, 7.1, 7.2, 7.1, 7.0, 7.2, 7.1],\r\n      itemStyle: { color: '#52c41a' }\r\n    },\r\n    {\r\n      name: '湿度',\r\n      type: 'line',\r\n      smooth: true,\r\n      data: [60, 62, 65, 63, 61, 64, 62],\r\n      itemStyle: { color: '#faad14' }\r\n    }\r\n  ]\r\n}))\r\n\r\n// 监测点列表\r\nconst monitorList = ref([\r\n  { name: '点A', type: '温度', value: '23.5°C', status: '正常' },\r\n  { name: '点B', type: '湿度', value: '60%', status: '正常' },\r\n  { name: '点C', type: 'pH值', value: '7.1', status: '异常' },\r\n  { name: '点D', type: '重金属', value: '0.03mg/L', status: '正常' }\r\n])\r\n\r\n// 搜索关键词\r\nconst searchKeyword = ref('')\r\n\r\n// 过滤后的监测点列表\r\nconst filteredMonitorList = computed(() => {\r\n  if (!searchKeyword.value) return monitorList.value\r\n  const keyword = searchKeyword.value.toLowerCase()\r\n  return monitorList.value.filter(item => \r\n    item.name.toLowerCase().includes(keyword) ||\r\n    item.type.toLowerCase().includes(keyword)\r\n  )\r\n})\r\n\r\n// 告警信息\r\nconst alerts = ref([\r\n  { \r\n    title: '监测点C pH值异常',\r\n    location: '区域A-C点',\r\n    time: '10分钟前',\r\n    level: 'warning'\r\n  },\r\n  { \r\n    title: '监测点F数据传输中断',\r\n    location: '区域B-F点',\r\n    time: '30分钟前',\r\n    level: 'error'\r\n  },\r\n  { \r\n    title: '监测点B湿度波动',\r\n    location: '区域A-B点',\r\n    time: '1小时前',\r\n    level: 'info'\r\n  }\r\n])\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.analysis-view {\r\n  min-height: 100vh;\r\n  padding: 24px;\r\n  background: linear-gradient(135deg, #001529 0%, #003366 100%);\r\n  color: #fff;\r\n}\r\n\r\n/* 顶部统计卡片样式 */\r\n.stats-section {\r\n  display: grid;\r\n  grid-template-columns: repeat(4, 1fr);\r\n  gap: 24px;\r\n  margin-bottom: 24px;\r\n}\r\n\r\n.stat-card {\r\n  background: rgba(255, 255, 255, 0.04);\r\n  border-radius: 8px;\r\n  padding: 20px;\r\n  display: flex;\r\n  align-items: center;\r\n  transition: all 0.3s;\r\n  backdrop-filter: blur(10px);\r\n  border: 1px solid rgba(255, 255, 255, 0.1);\r\n\r\n  &:hover {\r\n    transform: translateY(-2px);\r\n    background: rgba(255, 255, 255, 0.08);\r\n  }\r\n\r\n  .stat-icon {\r\n    width: 48px;\r\n    height: 48px;\r\n    border-radius: 8px;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    margin-right: 16px;\r\n    font-size: 24px;\r\n  }\r\n\r\n  .stat-content {\r\n    flex: 1;\r\n\r\n    .stat-value {\r\n      font-size: 24px;\r\n      font-weight: 600;\r\n      line-height: 1.2;\r\n      background: linear-gradient(120deg, #fff, #b7b7b7);\r\n      -webkit-background-clip: text;\r\n      -webkit-text-fill-color: transparent;\r\n    }\r\n\r\n    .stat-label {\r\n      font-size: 14px;\r\n      color: #8c8c8c;\r\n      margin-top: 4px;\r\n    }\r\n  }\r\n\r\n  .stat-trend {\r\n    display: flex;\r\n    align-items: center;\r\n    font-size: 12px;\r\n    padding: 4px 8px;\r\n    border-radius: 4px;\r\n    background: rgba(255, 255, 255, 0.04);\r\n\r\n    &.up {\r\n      color: #52c41a;\r\n    }\r\n\r\n    &.down {\r\n      color: #ff4d4f;\r\n    }\r\n\r\n    .el-icon {\r\n      margin-right: 4px;\r\n    }\r\n  }\r\n\r\n  &.primary .stat-icon {\r\n    background: rgba(24, 144, 255, 0.1);\r\n    color: #1890ff;\r\n  }\r\n\r\n  &.warning .stat-icon {\r\n    background: rgba(250, 173, 20, 0.1);\r\n    color: #faad14;\r\n  }\r\n\r\n  &.info .stat-icon {\r\n    background: rgba(24, 144, 255, 0.1);\r\n    color: #1890ff;\r\n  }\r\n\r\n  &.success .stat-icon {\r\n    background: rgba(82, 196, 26, 0.1);\r\n    color: #52c41a;\r\n  }\r\n}\r\n\r\n/* 主要内容区域样式 */\r\n.main-content {\r\n  display: grid;\r\n  grid-template-columns: 1fr 360px;\r\n  gap: 24px;\r\n  height: calc(100vh - 180px);\r\n}\r\n\r\n.content-main {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 24px;\r\n}\r\n\r\n/* 面板通用样式 */\r\n.monitor-panel,\r\n.trend-panel,\r\n.monitor-list-panel,\r\n.alert-panel {\r\n  background: rgba(255, 255, 255, 0.04);\r\n  border-radius: 8px;\r\n  padding: 20px;\r\n  backdrop-filter: blur(10px);\r\n  border: 1px solid rgba(255, 255, 255, 0.1);\r\n}\r\n\r\n.panel-header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  margin-bottom: 20px;\r\n\r\n  h3 {\r\n    margin: 0;\r\n    font-size: 18px;\r\n    font-weight: 500;\r\n  }\r\n\r\n  .panel-controls {\r\n    display: flex;\r\n    gap: 12px;\r\n  }\r\n}\r\n\r\n/* 实时数据网格样式 */\r\n.realtime-grid {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));\r\n  gap: 16px;\r\n}\r\n\r\n.realtime-item {\r\n  background: rgba(0, 0, 0, 0.2);\r\n  border-radius: 8px;\r\n  padding: 16px;\r\n  display: flex;\r\n  align-items: center;\r\n\r\n  .item-icon {\r\n    width: 40px;\r\n    height: 40px;\r\n    border-radius: 8px;\r\n    background: rgba(24, 144, 255, 0.1);\r\n    color: #1890ff;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    margin-right: 12px;\r\n    font-size: 20px;\r\n  }\r\n\r\n  .item-info {\r\n    flex: 1;\r\n\r\n    .item-label {\r\n      font-size: 14px;\r\n      color: #8c8c8c;\r\n      margin-bottom: 4px;\r\n    }\r\n\r\n    .item-value {\r\n      font-size: 18px;\r\n      font-weight: 500;\r\n\r\n      &.warning {\r\n        color: #faad14;\r\n      }\r\n\r\n      .warning-icon {\r\n        margin-left: 4px;\r\n        color: #faad14;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n/* 趋势图表样式 */\r