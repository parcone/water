{"ast":null,"code":"import { ref, reactive, computed, onMounted, onBeforeUnmount, watch } from 'vue';\nimport { DataLine, Warning, Location, Check, Refresh, Setting, Bell, ArrowUp, ArrowDown } from '@element-plus/icons-vue';\nimport BaseEChart from '@/components/BaseEChart.vue';\nimport PhotoTimeline from '@/components/PhotoTimeline.vue';\nimport AnimatedNumber from '@/components/AnimatedNumber.vue';\n\n// 统计数据\n\nexport default {\n  __name: 'Analysis',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const monitorStats = ref([{\n      value: 256,\n      label: '今日监测数据',\n      type: 'primary',\n      icon: DataLine,\n      trend: 12,\n      format: '0,0',\n      hoverText: '较昨日增长12%',\n      hover: false\n    }, {\n      value: 5,\n      label: '异常数据',\n      type: 'warning',\n      icon: Warning,\n      trend: -5,\n      format: '0,0',\n      hoverText: '较昨日下降5%',\n      hover: false\n    }, {\n      value: 32,\n      label: '监测点总数',\n      type: 'info',\n      icon: Location,\n      trend: 2,\n      format: '0,0',\n      hoverText: '新增2个监测点',\n      hover: false\n    }, {\n      value: 98,\n      label: '正常率',\n      type: 'success',\n      icon: Check,\n      trend: 3,\n      format: '0.00%',\n      hoverText: '正常率提升3%',\n      hover: false\n    }]);\n\n    // 监测点选择\n    const selectedPoint = ref('point1');\n    const monitorPoints = [{\n      label: '监测点A',\n      value: 'point1'\n    }, {\n      label: '监测点B',\n      value: 'point2'\n    }, {\n      label: '监测点C',\n      value: 'point3'\n    }];\n\n    // 实时数据\n    const realtimeData = ref([{\n      label: '温度',\n      value: 25.6,\n      icon: Location,\n      isAbnormal: false,\n      format: '0.0°C',\n      trend: '上升',\n      trendData: [23.1, 24.2, 24.8, 25.1, 25.6]\n    }, {\n      label: '湿度',\n      value: 65.2,\n      icon: Warning,\n      isAbnormal: true,\n      format: '0.0%',\n      trend: '稳定',\n      trendData: [65.0, 65.1, 65.2, 65.2, 65.2]\n    }, {\n      label: 'PH值',\n      value: 6.8,\n      icon: Check,\n      isAbnormal: false,\n      format: '0.0',\n      trend: '下降',\n      trendData: [7.2, 7.0, 6.9, 6.8, 6.8]\n    }]);\n\n    // 趋势分析配置\n    const trendTimeRange = ref('day');\n    const trendChartOptions = computed(() => ({\n      tooltip: {\n        trigger: 'axis',\n        axisPointer: {\n          type: 'cross'\n        }\n      },\n      legend: {\n        data: ['温度', '湿度', 'PH值']\n      },\n      grid: {\n        left: '3%',\n        right: '4%',\n        bottom: '3%',\n        containLabel: true\n      },\n      xAxis: {\n        type: 'category',\n        boundaryGap: false,\n        data: ['00:00', '06:00', '12:00', '18:00', '24:00']\n      },\n      yAxis: [{\n        type: 'value',\n        name: '温度(°C)/湿度(%)',\n        position: 'left'\n      }, {\n        type: 'value',\n        name: 'PH值',\n        position: 'right'\n      }],\n      series: [{\n        name: '温度',\n        type: 'line',\n        smooth: true,\n        data: [22, 23, 25, 26, 25],\n        itemStyle: {\n          color: '#ff7675'\n        }\n      }, {\n        name: '湿度',\n        type: 'line',\n        smooth: true,\n        data: [65, 64, 65, 66, 65],\n        itemStyle: {\n          color: '#74b9ff'\n        }\n      }, {\n        name: 'PH值',\n        type: 'line',\n        smooth: true,\n        yAxisIndex: 1,\n        data: [7.2, 7.0, 6.9, 6.8, 6.8],\n        itemStyle: {\n          color: '#55efc4'\n        }\n      }]\n    }));\n\n    // 监测点列表\n    const searchKeyword = ref('');\n    const filteredMonitorList = ref([{\n      name: '监测点A',\n      type: '农田',\n      value: '正常',\n      status: 'Normal',\n      trend: '稳定',\n      lastUpdate: '2分钟前'\n    }, {\n      name: '监测点B',\n      type: '水源',\n      value: '异常',\n      status: 'Warning',\n      trend: '上升',\n      lastUpdate: '1分钟前'\n    }]);\n\n    // 告警信息\n    const alerts = ref([{\n      title: '土壤湿度异常',\n      location: '监测点A',\n      time: '2小时前',\n      level: 'high'\n    }, {\n      title: 'PH值波动',\n      location: '监测点B',\n      time: '1天前',\n      level: 'medium'\n    }]);\n\n    // 交互方法\n    function refreshMonitorData() {\n      // 刷新数据逻辑\n    }\n    function openMonitorSettings() {\n      // 打开设置弹窗\n    }\n    function updateTrendChart() {\n      // 更新趋势图表\n    }\n    function filterMonitorList() {\n      // 根据搜索关键词过滤监测点\n    }\n    function selectMonitorPoint(item) {\n      // 选择监测点\n    }\n    function markAllAlertsRead() {\n      // 标记所有告警为已读\n    }\n    const __returned__ = {\n      monitorStats,\n      selectedPoint,\n      monitorPoints,\n      realtimeData,\n      trendTimeRange,\n      trendChartOptions,\n      searchKeyword,\n      filteredMonitorList,\n      alerts,\n      refreshMonitorData,\n      openMonitorSettings,\n      updateTrendChart,\n      filterMonitorList,\n      selectMonitorPoint,\n      markAllAlertsRead,\n      ref,\n      reactive,\n      computed,\n      onMounted,\n      onBeforeUnmount,\n      watch,\n      get DataLine() {\n        return DataLine;\n      },\n      get Warning() {\n        return Warning;\n      },\n      get Location() {\n        return Location;\n      },\n      get Check() {\n        return Check;\n      },\n      get Refresh() {\n        return Refresh;\n      },\n      get Setting() {\n        return Setting;\n      },\n      get Bell() {\n        return Bell;\n      },\n      get ArrowUp() {\n        return ArrowUp;\n      },\n      get ArrowDown() {\n        return ArrowDown;\n      },\n      BaseEChart,\n      PhotoTimeline,\n      AnimatedNumber\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","reactive","computed","onMounted","onBeforeUnmount","watch","DataLine","Warning","Location","Check","Refresh","Setting","Bell","ArrowUp","ArrowDown","BaseEChart","PhotoTimeline","AnimatedNumber","monitorStats","value","label","type","icon","trend","format","hoverText","hover","selectedPoint","monitorPoints","realtimeData","isAbnormal","trendData","trendTimeRange","trendChartOptions","tooltip","trigger","axisPointer","legend","data","grid","left","right","bottom","containLabel","xAxis","boundaryGap","yAxis","name","position","series","smooth","itemStyle","color","yAxisIndex","searchKeyword","filteredMonitorList","status","lastUpdate","alerts","title","location","time","level","refreshMonitorData","openMonitorSettings","updateTrendChart","filterMonitorList","selectMonitorPoint","item","markAllAlertsRead"],"sources":["D:/restore/water-melt/frontend/src/views/Analysis.vue"],"sourcesContent":["<!-- Analysis.vue -->\r\n<template>\r\n  <div class=\"analysis-view\">\r\n    <!-- 顶部统计卡片 -->\r\n    <div class=\"stats-section\">\r\n      <div \r\n        v-for=\"(stat, index) in monitorStats\" \r\n        :key=\"index\" \r\n        :class=\"['stat-card', stat.type]\"\r\n        @mouseenter=\"stat.hover = true\"\r\n        @mouseleave=\"stat.hover = false\"\r\n      >\r\n        <div class=\"stat-icon\">\r\n          <el-icon>\r\n            <component :is=\"stat.icon\" />\r\n          </el-icon>\r\n        </div>\r\n        <div class=\"stat-value\">\r\n          <AnimatedNumber \r\n            :value=\"stat.value\" \r\n            :format=\"stat.format || ''\"\r\n            :duration=\"1000\"\r\n          />\r\n        </div>\r\n        <div class=\"stat-label\">{{ stat.label }}</div>\r\n        <div \r\n          v-if=\"stat.trend\" \r\n          class=\"stat-trend\"\r\n          :class=\"stat.trend > 0 ? 'up' : 'down'\"\r\n        >\r\n          <el-icon>\r\n            <component :is=\"stat.trend > 0 ? ArrowUp : ArrowDown\" />\r\n          </el-icon>\r\n          <span>{{ Math.abs(stat.trend) }}%</span>\r\n        </div>\r\n        <div \r\n          v-if=\"stat.hover\" \r\n          class=\"stat-hover-info\"\r\n        >\r\n          {{ stat.hoverText }}\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 主要内容区域 -->\r\n    <div class=\"main-content\">\r\n      <!-- 左侧：实时监测和趋势图表 -->\r\n      <div class=\"content-main\">\r\n        <!-- 实时监测数据 -->\r\n        <div class=\"monitor-panel\">\r\n          <div class=\"panel-header\">\r\n            <h3>实时监测数据</h3>\r\n            <div class=\"panel-controls\">\r\n              <el-select \r\n                v-model=\"selectedPoint\" \r\n                size=\"small\" \r\n                placeholder=\"选择监测点\"\r\n                @change=\"refreshMonitorData\"\r\n              >\r\n                <el-option\r\n                  v-for=\"point in monitorPoints\"\r\n                  :key=\"point.value\"\r\n                  :label=\"point.label\"\r\n                  :value=\"point.value\"\r\n                />\r\n              </el-select>\r\n              <el-button-group>\r\n                <el-button \r\n                  size=\"small\" \r\n                  type=\"primary\" \r\n                  :icon=\"Refresh\"\r\n                  @click=\"refreshMonitorData\"\r\n                >\r\n                  刷新\r\n                </el-button>\r\n                <el-button \r\n                  size=\"small\" \r\n                  type=\"primary\" \r\n                  :icon=\"Setting\"\r\n                  @click=\"openMonitorSettings\"\r\n                >\r\n                  设置\r\n                </el-button>\r\n              </el-button-group>\r\n            </div>\r\n          </div>\r\n          <div class=\"realtime-grid\">\r\n            <div \r\n              v-for=\"(item, index) in realtimeData\" \r\n              :key=\"index\" \r\n              class=\"realtime-item\"\r\n            >\r\n              <div class=\"item-icon\">\r\n                <el-icon><component :is=\"item.icon\" /></el-icon>\r\n              </div>\r\n              <div class=\"item-info\">\r\n                <div class=\"item-label\">{{ item.label }}</div>\r\n                <div \r\n                  class=\"item-value\" \r\n                  :class=\"{ 'warning': item.isAbnormal }\"\r\n                >\r\n                  <AnimatedNumber \r\n                    :value=\"item.value\" \r\n                    :format=\"item.format || ''\"\r\n                    :duration=\"800\"\r\n                  />\r\n                  <el-tooltip \r\n                    v-if=\"item.isAbnormal\" \r\n                    effect=\"dark\" \r\n                    content=\"数值异常\" \r\n                    placement=\"top\"\r\n                  >\r\n                    <el-icon class=\"warning-icon\"><Warning /></el-icon>\r\n                  </el-tooltip>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 趋势分析图表 -->\r\n        <div class=\"trend-panel\">\r\n          <div class=\"panel-header\">\r\n            <h3>趋势分析</h3>\r\n            <div class=\"panel-controls\">\r\n              <el-radio-group \r\n                v-model=\"trendTimeRange\" \r\n                size=\"small\" \r\n                @change=\"updateTrendChart\"\r\n              >\r\n                <el-radio-button label=\"day\">24小时</el-radio-button>\r\n                <el-radio-button label=\"week\">7天</el-radio-button>\r\n                <el-radio-button label=\"month\">30天</el-radio-button>\r\n              </el-radio-group>\r\n            </div>\r\n          </div>\r\n          <div class=\"chart-container\">\r\n            <base-e-chart \r\n              :options=\"trendChartOptions\" \r\n              height=\"100%\" \r\n            />\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 右侧边栏：监测列表和告警信息 -->\r\n      <div class=\"content-side\">\r\n        <!-- 监测点列表 -->\r\n        <div class=\"monitor-list-panel\">\r\n          <div class=\"panel-header\">\r\n            <h3>监测点状态</h3>\r\n            <el-input\r\n              v-model=\"searchKeyword\"\r\n              size=\"small\"\r\n              placeholder=\"搜索监测点\"\r\n              prefix-icon=\"Search\"\r\n              style=\"width: 200px\"\r\n              @input=\"filterMonitorList\"\r\n            />\r\n          </div>\r\n          <div class=\"monitor-list\">\r\n            <div \r\n              v-for=\"item in filteredMonitorList\" \r\n              :key=\"item.name\" \r\n              class=\"monitor-item\"\r\n              @click=\"selectMonitorPoint(item)\"\r\n            >\r\n              <div class=\"monitor-info\">\r\n                <div class=\"monitor-name\">{{ item.name }}</div>\r\n                <div class=\"monitor-type\">{{ item.type }}</div>\r\n              </div>\r\n              <div class=\"monitor-value\">{{ item.value }}</div>\r\n              <div \r\n                class=\"monitor-status\" \r\n                :class=\"item.status.toLowerCase()\"\r\n              >\r\n                {{ item.status }}\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 告警信息 -->\r\n        <div class=\"alert-panel\">\r\n          <div class=\"panel-header\">\r\n            <h3>告警信息</h3>\r\n            <el-button \r\n              type=\"primary\" \r\n              size=\"small\" \r\n              :icon=\"Bell\"\r\n              @click=\"markAllAlertsRead\"\r\n            >\r\n              全部标记已读\r\n            </el-button>\r\n          </div>\r\n          <div class=\"alert-list\">\r\n            <div \r\n              v-for=\"(alert, index) in alerts\" \r\n              :key=\"index\" \r\n              class=\"alert-item\"\r\n              :class=\"alert.level\"\r\n            >\r\n              <div class=\"alert-icon\">\r\n                <el-icon><Warning /></el-icon>\r\n              </div>\r\n              <div class=\"alert-content\">\r\n                <div class=\"alert-title\">{{ alert.title }}</div>\r\n                <div class=\"alert-info\">\r\n                  <span>{{ alert.location }}</span>\r\n                  <span>{{ alert.time }}</span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 美化后的图片时间轴模块 -->\r\n    <div class=\"timeline-section\">\r\n      <PhotoTimeline />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, reactive, computed, onMounted, onBeforeUnmount, watch } from 'vue'\r\nimport { \r\n  DataLine, Warning, Location, Check, \r\n  Refresh, Setting, Bell, ArrowUp, ArrowDown \r\n} from '@element-plus/icons-vue'\r\nimport BaseEChart from '@/components/BaseEChart.vue'\r\nimport PhotoTimeline from '@/components/PhotoTimeline.vue'\r\nimport AnimatedNumber from '@/components/AnimatedNumber.vue'\r\n\r\n// 统计数据\r\nconst monitorStats = ref([\r\n  {\r\n    value: 256,\r\n    label: '今日监测数据',\r\n    type: 'primary',\r\n    icon: DataLine,\r\n    trend: 12,\r\n    format: '0,0',\r\n    hoverText: '较昨日增长12%',\r\n    hover: false\r\n  },\r\n  {\r\n    value: 5,\r\n    label: '异常数据',\r\n    type: 'warning',\r\n    icon: Warning,\r\n    trend: -5,\r\n    format: '0,0',\r\n    hoverText: '较昨日下降5%',\r\n    hover: false\r\n  },\r\n  {\r\n    value: 32,\r\n    label: '监测点总数',\r\n    type: 'info',\r\n    icon: Location,\r\n    trend: 2,\r\n    format: '0,0',\r\n    hoverText: '新增2个监测点',\r\n    hover: false\r\n  },\r\n  {\r\n    value: 98,\r\n    label: '正常率',\r\n    type: 'success',\r\n    icon: Check,\r\n    trend: 3,\r\n    format: '0.00%',\r\n    hoverText: '正常率提升3%',\r\n    hover: false\r\n  }\r\n])\r\n\r\n// 监测点选择\r\nconst selectedPoint = ref('point1')\r\nconst monitorPoints = [\r\n  { label: '监测点A', value: 'point1' },\r\n  { label: '监测点B', value: 'point2' },\r\n  { label: '监测点C', value: 'point3' }\r\n]\r\n\r\n// 实时数据\r\nconst realtimeData = ref([\r\n  { \r\n    label: '温度', \r\n    value: 25.6, \r\n    icon: Location, \r\n    isAbnormal: false,\r\n    format: '0.0°C',\r\n    trend: '上升',\r\n    trendData: [23.1, 24.2, 24.8, 25.1, 25.6]\r\n  },\r\n  { \r\n    label: '湿度', \r\n    value: 6