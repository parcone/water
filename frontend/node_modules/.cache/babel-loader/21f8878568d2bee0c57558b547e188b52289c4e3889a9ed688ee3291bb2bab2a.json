le = false\r\n      previewDialog.data = {}\r\n    }\r\n\r\n    return {\r\n      filters,\r\n      tableData,\r\n      total,\r\n      pageSize,\r\n      currentPage,\r\n      previewDialog,\r\n      handleSizeChange,\r\n      handleCurrentChange,\r\n      viewImage,\r\n      deleteImage,\r\n      handleDialogClose\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.data-management {\r\n  padding: 20px;\r\n  height: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 20px;\r\n  background-color: #001529;\r\n}\r\n\r\n.filter-bar {\r\n  display: flex;\r\n  gap: 15px;\r\n  flex-wrap: wrap;\r\n  padding: 20px;\r\n  background: rgba(255, 255, 255, 0.04);\r\n  border-radius: 4px;\r\n\r\n  .filter-item {\r\n    min-width: 200px;\r\n  }\r\n}\r\n\r\n.data-table {\r\n  flex: 1;\r\n  background: rgba(255, 255, 255, 0.04);\r\n  border-radius: 4px;\r\n  padding: 20px;\r\n\r\n  :deep(.el-table) {\r\n    background-color: transparent;\r\n    color: #fff;\r\n\r\n    th {\r\n      background-color: #1a1a1a;\r\n      color: #fff;\r\n      border-bottom: 1px solid #303030;\r\n    }\r\n\r\n    td {\r\n      background-color: transparent;\r\n      border-bottom: 1px solid #303030;\r\n    }\r\n\r\n    tr:hover > td {\r\n      background-color: rgba(255, 255, 255, 0.04);\r\n    }\r\n  }\r\n}\r\n\r\n.pagination {\r\n  display: flex;\r\n  justify-content: center;\r\n  padding: 20px;\r\n  background: rgba(255, 255, 255, 0.04);\r\n  border-radius: 4px;\r\n}\r\n\r\n.image-preview {\r\n  display: grid;\r\n  grid-template-columns: 300px 1fr;\r\n  gap: 20px;\r\n  height: 600px;\r\n\r\n  .preview-info {\r\n    padding: 20px;\r\n    background: rgba(0, 0, 0, 0.2);\r\n    border-radius: 4px;\r\n\r\n    h3 {\r\n      margin: 0 0 20px;\r\n      color: #fff;\r\n    }\r\n\r\n    .info-item {\r\n      margin: 10px 0;\r\n      color: #fff;\r\n\r\n      .label {\r\n        color: #8c8c8c;\r\n        margin-right: 10px;\r\n      }\r\n    }\r\n  }\r\n\r\n  .preview-image {